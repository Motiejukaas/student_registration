<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.student_registration.control.AddStudentController">
    <ChoiceBox fx:id="group_choice_box" prefWidth="200.0">
        <!-- Populate the choice box with available group options -->
    </ChoiceBox>
    <TextField fx:id="name_text_field" promptText="Enter student name"/>
    <TextField fx:id="surname_text_field" promptText="Enter student surname"/>
    <TextField fx:id="student_id_text_field" promptText="Enter student ID"/>
    <Button fx:id="submit_button" onAction="#submitData" text="Submit"/>
</VBox>
