<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="200.0" prefWidth="235.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.student_registration.control.AddStudentController">
   <AnchorPane prefHeight="200.0" prefWidth="236.0">
      <children>
          <TextField fx:id="surname_text_field" layoutX="18.0" layoutY="81.0" prefHeight="26.0" prefWidth="200.0" promptText="Enter student surname" />
          <ChoiceBox fx:id="group_choice_box" layoutX="18.0" layoutY="14.0" prefWidth="200.0">
    </ChoiceBox>
          <TextField fx:id="name_text_field" layoutX="18.0" layoutY="47.0" prefHeight="26.0" prefWidth="200.0" promptText="Enter student name" />
          <TextField fx:id="student_id_text_field" layoutX="18.0" layoutY="121.0" prefHeight="26.0" prefWidth="200.0" promptText="Enter student ID" />
          <Button fx:id="submit_button" layoutX="86.0" layoutY="156.0" onAction="#submitData" text="Submit">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
      </children>
   </AnchorPane>
</VBox>
