<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="141.0" prefWidth="229.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.student_registration.control.AddGroupController">
   <AnchorPane prefHeight="140.0" prefWidth="242.0">
      <children>
          <Button fx:id="submit_button" layoutX="82.0" layoutY="92.0" onAction="#submitData" text="Submit">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
          <TextField fx:id="group_name_text_field" layoutX="14.0" layoutY="51.0" prefHeight="26.0" prefWidth="200.0" promptText="Enter group name" />
          <ChoiceBox fx:id="group_choice_box" layoutX="14.0" layoutY="14.0" prefWidth="200.0">
              <items>
                  <FXCollections fx:factory="observableArrayList">
                      <String fx:value="New Group" />
                  </FXCollections>
              </items>
          </ChoiceBox>
      </children>
   </AnchorPane>
</VBox>
